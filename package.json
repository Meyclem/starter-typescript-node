{
  "name": "starter-typescript-node",
  "version": "0.0.1",
  "description": "A starter project for TypeScript and Node.js",
  "author": "Meyclem",
  "keywords": [
    "starter",
    "typescript",
    "nodejs"
  ],
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .tsbuildinfo src/types",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest run --coverage --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@stylistic/eslint-plugin": "5.6.1",
    "@types/node": "25.0.1",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.49.0",
    "@vitest/coverage-v8": "4.0.15",
    "@vitest/ui": "4.0.15",
    "eslint": "9.39.2",
    "eslint-plugin-import": "2.32.0",
    "globals": "16.5.0",
    "ts-node": "10.9.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.15"
  }
}
